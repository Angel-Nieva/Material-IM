library(ggpubr)
library(dplyr)
# Definir semilla
set.seed(7)
# ******************************************** PREGUNTA 1 ********************************#
# Si el ingeniero está seguro de que el verdadero volumen medio no puede ser inferior     #
# a 10 litros y piensa rechazar la hipótesis nula cuando la muestra presente una          #
# media mayor a 10,3 litros, ¿cuál es la probabilidad de que cometa un error de tipo I?   #                                               #
# ****************************************************************************************#
# Fijar valores conocidos .
sigma <- 1
n <- 100
media_nula <- 10
# Calcular el error estándar .
SE <- sigma / sqrt (n)
# Graficar la distribución muestral de la media de las diferencias si
# la hipótesis nula fuera verdadera.
#x <- seq (9 , 11 , 0.01)
x <- seq(8,12,0.01)
y <- dnorm (x, mean = media_nula , sd = SE)
g <- ggplot ( data = data.frame (x, y), aes(x))
g <- g + stat_function (
fun = dnorm ,
args = list ( mean = media_nula , sd = SE),
colour = "red ", size = 1)
g <- g + ylab ("")
g <- g + scale_y_continuous ( breaks = NULL )
g <- g + scale_x_continuous ( name = " Diferencia en tiempos de ejecuci ón [ms]",
breaks = seq (8, 12, 1))
g <- g + theme_pubr ()
print(g)
g <- g + geom_area ( data = subset (df , x > q_critico_superior ),
aes (y = y),
colour = "red ",
fill = "red")
